{
    "name": "honda/adev-navigation",
    "description": "navigation",
    "keywords": [
        "honda",
        "navigation"
    ],
    "homepage": "https://github.com/honda/navigation",
    "license": "MIT",
    "authors": [
        {
            "name": "FÃ©lix Dorn",
            "email": "github@felixdorn.fr",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "honda/url-pattern-matcher": "^1.0.1",
        "honda/url-resolver": "1.0.0",
        "illuminate/contracts": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3",
        "orchestra/testbench": "^6.13",
        "pestphp/pest": "^1.0.5",
        "phpstan/phpstan": "^1.7.6",
        "symfony/var-dumper": "^5.2.0"
    },
    "autoload": {
        "psr-4": {
            "Honda\\Navigation\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Honda\\Navigation\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "lint": "php-cs-fixer fix -v",
        "test:lint": "php-cs-fixer fix -v --dry-run",
        "test:types": "phpstan analyse --ansi --memory-limit=0",
        "test:unit": "pest --colors=always",
        "test": [
            "@test:lint",
            "@test:types",
            "@test:unit"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Honda\\Navigation\\NavigationServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
